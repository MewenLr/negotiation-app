{
  "name": "ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "TARGET_ENV='client' vue-cli-service build --no-clean",
    "build:server": "TARGET_ENV='server' vue-cli-service build --no-clean",
    "dev": "vue-cli-service serve",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "vue-cli-service lint ./",
    "lint:style": "stylelint ./**/*.vue ./**/*.sass ./**/*.scss --fix",
    "postbuild": "rm ./dist/index.html",
    "prod": "npm run build && npm run ssr",
    "ssr": "node ./server/ssr/index.js",
    "proxy": "node ./server/proxy/index.js",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0",
    "stylelint": "^13.3.3",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.17.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.1.3",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^0.17.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": ">=5.3.0",
    "@storybook/addon-knobs": ">=5.3.0",
    "@storybook/addon-links": ">=5.3.0",
    "@storybook/addon-notes": ">=5.3.0",
    "@storybook/vue": ">=5.3.0",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "axios-mock-adapter": "^1.18.1",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "flush-promises": "^1.0.2",
    "hash-sum": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^9.5.0",
    "loader-utils": "^2.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "storybook-vue-router": "^1.0.7",
    "svg-sprite-loader": "^5.0.0",
    "svgo-loader": "^2.2.1",
    "typescript": "~3.8.3",
    "vue-cli-plugin-storybook": "~1.2.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^4.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
